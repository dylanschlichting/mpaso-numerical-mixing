<streams>
<immutable_stream name="mesh"
                  type="input"
                  filename_template="channel_init.nc"
                  input_interval="initial_only" />

<immutable_stream name="input"
                  type="input"
                  filename_template="channel_init.nc"
                  input_interval="initial_only" />

<immutable_stream name="restart"
                  type="input;output"
                  filename_template="restarts/restart.$Y-$M-$D_$h.$m.$s.nc"
                  filename_interval="output_interval"
                  reference_time="0001-01-01_00:00:00"
                  clobber_mode="truncate"
                  input_interval="initial_only"
                  output_interval="0005_00:00:00" />

<stream name="output"
        type="output"
        filename_template="output.nc"
        filename_interval="01-00-00_00:00:00"
        reference_time="0001-01-01_00:00:00"
        clobber_mode="truncate"
        precision="double"
        output_interval="0000-00-00_00:00:01" >

        <var name="xtime"/>
        <var_struct name="tracers"/>
        <var name="layerThickness"/>
        <var name="ssh"/>
        <var name="normalVelocity"/>
        <var name="kineticEnergyCell"/>
        <var name="relativeVorticityCell"/>
        <var name="divergence"/>
        <var name="velocityZonal"/>
	<var name="velocityMeridional"/>
        <var_array name="activeTracersTend"/>
        <var_array name="activeTracerHorizontalAdvectionTendency"/>
        <var_array name="activeTracerVerticalAdvectionTendency"/>
        <var_array name="activeTracerVertMixTendency"/>
        <var_array name="activeTracerHorMixTendency"/>
        <var_array name="activeTracerSurfaceFluxTendency"/>
</stream>

<stream name="discreteVarianceDecayOutput"
        type="output"
        filename_template="analysis_members/discreteVarianceDecay.$Y-$M-$D.nc"
        filename_interval="01-00-00_00:00:00"
        reference_time="0001-01-01_00:00:00"
        clobber_mode="truncate"
        precision="double"
        packages="discreteVarianceDecayAMPKG"
        output_interval="0000-00-00_00:00:01" >

        <var name="xtime"/>
        <var_array name="chiSpurTracerBR08"/>
        <var_array name="chiPhyVerTracer"/>
</stream>

</streams>
